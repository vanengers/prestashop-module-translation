{
    "name": "vanengers/prestashop-module-translation",
    "description": "Extract and translate into xlf files",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vanengers\\PrestashopModuleTranslation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanengers\\PrestashopModuleTranslation\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "George van Engers",
            "email": "vanengers@gmail.com"
        }
    ],
    "require": {
        "prestashop/module-translation-tool": "@dev",
        "deeplcom/deepl-php": "dev-main",
        "vanengers/symfony-console-command-lib": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/PrestaShopCorp/module-translation-tool.git",
            "symlink": false
        }
    ],
    "bin": [
        "bin/extract"
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev"
}
